                    <script>
                        const rawText = "Scroll Less, Move Fast – Claim It First";
                        const highlightStart = 29; // index of "Claim It First"
                        const target = document.getElementById("typewriter");

                        let index = 0;
                        function typeWriter() {
                          if (index < rawText.length) {
                            const currentChar = document.createTextNode(rawText.charAt(index));
                            target.appendChild(currentChar);
                            index++;
                            setTimeout(typeWriter, 40);
                          } else {
                            // Wrap the highlight portion after typing is done
                            const fullText = target.textContent;
                            const before = fullText.substring(0, highlightStart);
                            const highlight = fullText.substring(highlightStart);

                            target.innerHTML = `${before}<span class="highlight">${highlight}</span>`;
                          }
                        }

                        window.addEventListener("DOMContentLoaded", () => {
                          setTimeout(typeWriter, 700);
                        });
                    </script>


