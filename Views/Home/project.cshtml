    .animated-tagline-wrapper {
        display: flex;
        justify-content: flex-start;
        margin-top: 1rem;
    }

    .animated-tagline {
        font-size: 18px;
        font-weight: 600;
        color: white;
        opacity: 0;
        animation: fadeSlideIn 1.2s ease-out forwards;
        animation-delay: 0.5s;
        position: relative;
        min-height: 36px; /* Increased from 28px */
        overflow: visible; /* Important fix */
        line-height: 1.6; /* Extra spacing for bounce */
    }


    .shine-text {
        display: inline-block;
        position: relative;
        animation: pulseBounce 2s infinite ease-in-out;
    }

    .highlight {
    }

    .shine-text::before {
        content: '';
        position: absolute;
        top: 0;
        left: -75%;
        height: 100%;
        width: 70%;
        background: linear-gradient( 120deg, rgba(255, 255, 255, 0.1) 0%, rgba(255, 255, 255, 0.5) 50%, rgba(255, 255, 255, 0.1) 100% );
        transform: skewX(-20deg);
        animation: shine 2.5s infinite ease-in-out;
        z-index: 1;
    }

    @@keyframes fadeSlideIn {
        0% {
            opacity: 0;
            transform: translateY(15px);
        }

        100% {
            opacity: 1;
            transform: translateY(0);
        }
    }

    @@keyframes pulseBounce {
        0%, 100% {
            transform: scale(1);
        }

        50% {
            transform: scale(1.05) translateY(-4px);
        }
    }

    @@keyframes shine {
        0% {
            left: -75%;
        }

        100% {
            left: 125%;
        }
    }
</style>
